<p id="notice"><%= notice %></p>

<h1>Station Histories</h1>

<table>
  <thead>
    <tr>
      <th>Free slots</th>
      <th>Free bikes</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @station_histories.each do |station_history| %>
      <tr>
        <td><%= station_history.free_slots %></td>
        <td><%= station_history.free_bikes %></td>
        <td><%= station_history.rate %></td>
        <td><%= link_to 'Show', station_history %></td>
        <td><%= link_to 'Edit', edit_station_history_path(station_history) %></td>
        <td><%= link_to 'Destroy', station_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station History', new_station_history_path %>
